eters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <h1 class=\"mbr-fonts-style align-center mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">SHARE THIS PAGE!</h1>\n\n    <div class=\"ico-wrapper\">\n\n        <amp-social-share type=\"email\" mbr-if=\"email\">\n            <span class=\"iconfont-wrapper\">\n                <span mbr-icon class=\"amp-iconfont fa-youtube-play fa\"></span>\n            </span>\n        </amp-social-share>\n        <amp-social-share type=\"facebook\" mbr-if=\"facebook\">\n            <span class=\"iconfont-wrapper\">\n                <span mbr-icon class=\"amp-iconfont fa-facebook fa\"></span>\n            </span>\n        </amp-social-share>\n        <amp-social-share type=\"gplus\" mbr-if=\"gplus\">\n            <span class=\"iconfont-wrapper\">\n                <span mbr-icon class=\"amp-iconfont fa-google-plus fa\"></span>\n            </span>\n        </amp-social-share>\n        <amp-social-share type=\"linkedin\" mbr-if=\"linkedin\">\n            <span class=\"iconfont-wrapper\">\n                <span mbr-icon class=\"amp-iconfont fa-linkedin fa\"></span>\n            </span>\n        </amp-social-share>\n        <amp-social-share type=\"pinterest\" mbr-if=\"pinterest\">\n            <span class=\"iconfont-wrapper\">\n                <span mbr-icon class=\"amp-iconfont fa-pinterest-p fa\"></span>\n            </span>\n        </amp-social-share>\n        <amp-social-share type=\"tumblr\" mbr-if=\"tumblr\">\n            <span class=\"iconfont-wrapper\">\n                <span mbr-icon class=\"amp-iconfont fa-tumblr fa\"></span>\n            </span>\n        </amp-social-share>\n        <amp-social-share type=\"twitter\" mbr-if=\"twitter\">\n            <span class=\"iconfont-wrapper\">\n                <span mbr-icon class=\"amp-iconfont fa-twitter fa\"></span>\n            </span>\n      